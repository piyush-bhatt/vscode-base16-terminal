{
  "name": "base16-terminal",
  "displayName": "Base16 Terminal",
  "description": "Base16 colour themes for VSCode terminal",
  "version": "1.0.0",
  "publisher": "piyush-bhatt",
  "author": {
    "name": "Piyush Bhatt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/piyush-bhatt/vscode-base16-terminal"
  },
  "bugs": {
    "url": "https://github.com/piyush-bhatt/vscode-base16-terminal/issues"
  },
  "homepage": "https://github.com/piyush-bhatt/vscode-base16-terminal#readme",
  "license": "SEE LICENSE IN LICENSE",
  "keywords": [
    "base16",
    "terminal",
    "color",
    "color-theme",
    "theme"
  ],
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onCommand:base16-term.setTerminalTheme"
  ],
  "main": "./dist/extension.js",
  "icon": "media/icon/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "base16-term.setTerminalTheme",
        "title": "Set Terminal Theme"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./dist/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "ts-loader": "^8.0.14",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "printWidth": 120,
    "endOfLine": "auto",
    "formatOnSave": true
  }
}
